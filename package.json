{
  "name": "weather-sounds",
  "version": "1.0.0",
  "description": "Nature sounds app with volume control and background changes",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run lint:watch\" \"webpack serve --config webpack.config.dev.js\"",
    "lint": "eslint .",
    "lint:watch": "onchange \"src/**/*.{js,jsx,ts,tsx}\" -- npm run lint",
    "build": "npx webpack --config webpack.config.prod.js",
    "build:dev": "npx webpack --config webpack.config.dev.js"
  },
  "keywords": [
    "webpack",
    "js"
  ],
  "author": "Anastasiya Guryanova",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.0.1",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.13.0",
    "eslint-import-resolver-webpack": "^0.13.9",
    "html-webpack-plugin": "^5.6.3",
    "onchange": "^7.1.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  }
}
